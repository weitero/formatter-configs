{
  "$schema": "https://www.schemastore.org/prettierrc.json",
  "checkIgnorePragma": true,
  "experimentalOperatorPosition": "start",
  "overrides": [
    {
      "files": [
        "*.css",
        "*.js",
        "*.jsx"
      ],
      "options": {
        "singleQuote": true
      }
    }
  ],
  "printWidth": 120,
  "proseWrap": "always",
  "quoteProps": "consistent",
  "trailingComma": "es5",
  "vueIndentScriptAndStyle": true
}
